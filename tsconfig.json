{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES5"],
    "declaration": true,
    "outDir": "dist/",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "experimentalDecorators": true
  },
  "include": [
    "src/"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}